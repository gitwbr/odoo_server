			<tree editable="bottom">
				<field name="name"/>
				<field name="is_disabled"/>
				<!-- <field name="worktype_ids"  widget="many2many_tags"/> -->
			</tree>
		</field>
    </record>
	
	<record model="ir.ui.view" id="dtsc_reworklist_tree">
		<field name="name">dtsc.reworklist.tree</field>
		<field name="model">dtsc.reworklist</field>
		<field name="arch" type="xml">
			<tree editable="bottom">
				<field name="name"/>
				<field name="is_disabled"/>
			</tree>
		</field>
    </record>
