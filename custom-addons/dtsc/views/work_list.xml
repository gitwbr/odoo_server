<odoo>
	
	<record model="ir.ui.view" id="dtsc_userlist_tree">
		<field name="name">dtsc.userlist.tree</field>
		<field name="model">dtsc.userlist</field>
		<field name="arch" type="xml">
			<tree editable="bottom">
				<field name="name"/>
				<field name="worktype_ids"  widget="many2many_tags"/>
				<field name="is_disabled"/>
			</tree>
		</field>
    </record>
	
	
	<record model="ir.ui.view" id="dtsc_userlistbefore_tree">
		<field name="name">dtsc.userlistbefore.tree</field>
		<field name="model">dtsc.userlistbefore</field>
